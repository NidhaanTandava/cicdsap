	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
                        <VBox alignContent="Center" justifyContent="Begin" alignItems="Center">
                            <Title level="h2" class="sapUiMediumMarginBeginEnd sapUiMediumMarginTop" text="CI CD Process" />
